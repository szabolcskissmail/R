if (file.exists(filename)) file.remove(filename)
pos <- 0
posEOL <- 0
for (i in dna) {
cat(i, file=filename, append=TRUE)
if(posEOL == 5){
cat('\n', file=filename, append=TRUE)
posEOL <- 0
} else {
if(pos == 9){
cat(' ', file=filename, append=TRUE)
pos <- 0
posEOL <- posEOL + 1
} else {
pos <- pos + 1
}
}
}
}
